<% include ./partials/header %>
<div class="data-table">
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Password</th>
      </tr>
    </thead>
    <tbody>
      <% if(data.length){
       for(var i = 0;i < data.length;i++) { %>
        <tr>
          <td><%=(i+1)%></td>
          <td><%=data[i].name%></td>
          <td><%=data[i].email%></td>
          <td><%=data[i].password%></td>
        </tr>
      <% } } else{ %>
       <tr>
          <td>No Data</td>
       </tr>
      <% } %>
    </tbody>
  </table>
</div>

<% include ./partials/footer %>
